     -->

    


    <!-- http://purl.org/vocab/frbr/core#creator -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#creator">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#creatorOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:comment xml:lang="it">Lega l&apos;entita all&apos;opera, cioè chi realizza quell&apos;opera</rdfs:comment>
        <rdfs:label xml:lang="en">creator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#creatorOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#creatorOf">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">Indica da chi è stato creato il brano musciale</rdfs:comment>
        <rdfs:label xml:lang="en">creatorOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#embodiment -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#embodiment">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#embodimentOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:comment xml:lang="it">L&apos;espressione si materializzazione (embodiment) in una manifestazione.</rdfs:comment>
        <rdfs:label xml:lang="en">embodiment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#embodimentOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#embodimentOf">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:comment xml:lang="it">Rappresenta la tokenizzazione astratta di un&apos;opera artistica.</rdfs:comment>
        <rdfs:label xml:lang="en">embodimentOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#exemplar -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#exemplar">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#exemplarOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
        <rdfs:comment xml:lang="it">Il singolo esemplare di una manifestazione è l&apos;item.</rdfs:comment>
        <rdfs:label xml:lang="en">exemplar</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#exemplarOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#exemplarOf">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:comment xml:lang="it">L&apos;item è il singolo esemplare di una manifestazione</rdfs:comment>
        <rdfs:label xml:lang="en">exemplarOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#isAbstracted -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#isAbstracted">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#itIsAnAbstraction"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:comment xml:lang="it">Indica l&apos;opera in cosa si astrae (espressione)</rdfs:comment>
        <rdfs:label xml:lang="en">isAbstracted</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#itIsAnAbstraction -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#itIsAnAbstraction">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:comment xml:lang="it">Indica l&apos;opera che quell&apos;espressione sta astraendo.</rdfs:comment>
        <rdfs:label xml:lang="en">itIsAnAbstraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#owener -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#owener">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#owenerOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
        <rdfs:comment xml:lang="it">Indica quali item possiede l&apos;entità responsabile</rdfs:comment>
        <rdfs:label xml:lang="en">owener</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#owenerOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#owenerOf">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">Indica da quale entità responsabilel&apos;item è posseduto</rdfs:comment>
        <rdfs:label xml:lang="en">owenerOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#producer -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#producer">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#producerOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:comment xml:lang="it">Indica quale manifestazione l&apos;entità responsabile ha prodotto</rdfs:comment>
        <rdfs:label xml:lang="en">producer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#producerOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#producerOf">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">Indica la manifestazione da quale entità responsabile è stata prodotta</rdfs:comment>
        <rdfs:label xml:lang="en">producerOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#realizer -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#realizer">
        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#realizerOf"/>
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:comment xml:lang="it">Indica l&apos;espressione realizzata dall&apos;entità responsabile</rdfs:comment>
        <rdfs:label xml:lang="en">realizer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#realizerOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#realizerOf">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">Indica da quale entità responsabile è stata realizzata l&apos;espressione</rdfs:comment>
        <rdfs:label xml:lang="en">realizerOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Album"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:comment xml:lang="it">Relazione che lega l&apos;album al brano musicale. Indica quindi da quali brani musicali è composto l&apos;album</rdfs:comment>
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
        <rdfs:label xml:lang="it">èCompostoDa</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Album"/>
        <rdfs:comment xml:lang="it">Relazione che lega il brano musicale all&apos;album a cui appartiene.</rdfs:comment>
        <rdfs:label xml:lang="it">faParteDi</rdfs:label>
        <rdfs:label xml:lang="en">isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#albumOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#albumOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#hasAlbum"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Album"/>
        <rdfs:range rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">Indica a quale artista appartiene un determinato album</rdfs:comment>
        <rdfs:label xml:lang="it">albumDi</rdfs:label>
        <rdfs:label xml:lang="en">albumOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#composedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#composedBy">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#creatorOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#composes"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Composition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Composer"/>
        <rdfs:comment xml:lang="it">Lega la composizione con il compositore. Indica dunque da chi è stato composta una composizione</rdfs:comment>
        <rdfs:label xml:lang="en">composedBy</rdfs:label>
        <rdfs:label xml:lang="it">compostaDa</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#composes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#composes">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#creator"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Composer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Composition"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.com/MusicOntology#sing"/>
        <rdfs:comment xml:lang="it">Artista compositore che compone un brano musciale</rdfs:comment>
        <rdfs:label xml:lang="it">compone</rdfs:label>
        <rdfs:label xml:lang="en">composes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#consistOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#consistOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#formAGroup"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Group"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:comment xml:lang="it">Specifica da quali artisti è composto il gruppo</rdfs:comment>
        <rdfs:label xml:lang="en">consistOf</rdfs:label>
        <rdfs:label xml:lang="it">formatoDa</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#containedInLibrary -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#containedInLibrary">
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#libraryContains"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary"/>
        <rdfs:comment xml:lang="it">Il token di un&apos;opera è contenuto in una libreria musicale.</rdfs:comment>
        <rdfs:label xml:lang="en">containedInLibrary</rdfs:label>
        <rdfs:label xml:lang="it">contenutoInLibreria</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#featuring -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#featuring">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Singer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Singer"/>
        <rdfs:comment xml:lang="it">Lega più artisti che collaborano nella realizzazione di una canzone(featuring)</rdfs:comment>
        <rdfs:label xml:lang="it">collabora</rdfs:label>
        <rdfs:label xml:lang="en">featuring</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#formAGroup -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#formAGroup">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Group"/>
        <rdfs:comment xml:lang="it">Lega l&apos;artista al gruppo musicale a cui appartiene</rdfs:comment>
        <rdfs:label xml:lang="en">formAGroup</rdfs:label>
        <rdfs:label xml:lang="it">forma</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#groupSing -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#groupSing">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#creator"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#singedByGroup"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Group"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:comment xml:lang="it">Gruppo musicale che canta una canzone</rdfs:comment>
        <rdfs:label xml:lang="en">groupSing</rdfs:label>
        <rdfs:label xml:lang="it">gruppoCanta</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#hasAlbum -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#hasAlbum">
        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Album"/>
        <rdfs:comment xml:lang="it">Indica quale album e posseduto da un artista o un gruppo composto da più artisti</rdfs:comment>
        <rdfs:label xml:lang="it">haAlbum</rdfs:label>
        <rdfs:label xml:lang="en">hasAlbum</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#hasGenre -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#hasGenre">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Genre"/>
        <rdfs:comment xml:lang="it">Proprietà che indica il genere della canzone</rdfs:comment>
        <rdfs:label xml:lang="en">hasGenre</rdfs:label>
        <rdfs:label xml:lang="it">èDiGenere</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#hasLibrary -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#hasLibrary">
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#libraryOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#User"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary"/>
        <rdfs:comment xml:lang="it">Proprietà che indica la libreria musicale posseduta da un determianto utente</rdfs:comment>
        <rdfs:label xml:lang="it">haLibreria</rdfs:label>
        <rdfs:label xml:lang="en">hasLibrary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#hasSongs -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#hasSongs">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Genre"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:comment xml:lang="it">Relazione mi permette di determinare quali brani musicali appartengono ad un determinato genere musicale.</rdfs:comment>
        <rdfs:label xml:lang="it">haCanzoni</rdfs:label>
        <rdfs:label xml:lang="en">hasSongs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#hasToken -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#hasToken">
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#tokenOfUser"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:comment xml:lang="it">Indica quali token un utente possiede</rdfs:comment>
        <rdfs:label xml:lang="it">haToken</rdfs:label>
        <rdfs:label xml:lang="en">hasToken</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#libraryContains -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#libraryContains">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:comment xml:lang="it">Una libreria musicale contiene un token opera, che può essere un album, un brano musicale o una composizione.</rdfs:comment>
        <rdfs:label xml:lang="en">libraryContains</rdfs:label>
        <rdfs:label xml:lang="it">libreriaContiene</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#libraryHas -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#libraryHas">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#libraryContains"/>
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#tokenAlbumContains"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Property chain: se una libreria contiene un token opera e questo token opera è un album, l&apos;album a sua volta conterra un token brano musicale, quindi inferiamo che la libreria musicale HA quel/i brano/i musicale/i.</rdfs:comment>
        <rdfs:label xml:lang="en">libraryHas</rdfs:label>
        <rdfs:label xml:lang="it">libreriaHa</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#libraryOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#libraryOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#User"/>
        <rdfs:comment xml:lang="it">Indica a chi appartiene la libreria musicale</rdfs:comment>
        <rdfs:label xml:lang="en">libraryOf</rdfs:label>
        <rdfs:label xml:lang="it">libreriaDi</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#sing -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#sing">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#creator"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#singedBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Singer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:comment xml:lang="it">Artista (solista o di gruppo) che canta la melodia principale di un brano musicale</rdfs:comment>
        <rdfs:label xml:lang="it">canta</rdfs:label>
        <rdfs:label xml:lang="en">sing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#singedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#singedBy">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#creatorOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Singer"/>
        <rdfs:comment xml:lang="it">Ci dice da chi è stato cantato un brano musciale</rdfs:comment>
        <rdfs:label xml:lang="it">cantataDa</rdfs:label>
        <rdfs:label xml:lang="en">singedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#singedByGroup -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#singedByGroup">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/vocab/frbr/core#creatorOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#Group"/>
        <rdfs:comment xml:lang="it">Relazione che lega la canzone con il gruppo musicale, quindi chi canta quella canzone</rdfs:comment>
        <rdfs:label xml:lang="it">cantataDaGruppo</rdfs:label>
        <rdfs:label xml:lang="en">singedByGroup</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#tokenAlbumContains -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#tokenAlbumContains">
        <owl:inverseOf rdf:resource="http://www.semanticweb.com/MusicOntology#tokenSongContainedIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:comment xml:lang="it">Relazione utilizzata solo nel caso degli album. In questo specifico caso un album contiene a sua volta il token delle canzoni.</rdfs:comment>
        <rdfs:label xml:lang="it">tokenAlbumContiene</rdfs:label>
        <rdfs:label xml:lang="en">tokenContains</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#tokenOfUser -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#tokenOfUser">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#User"/>
        <rdfs:comment xml:lang="it">Indica uno specifico token di una canzone a quale utente appartiene</rdfs:comment>
        <rdfs:label xml:lang="it">tokenDi</rdfs:label>
        <rdfs:label xml:lang="en">tokenOfUser</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#tokenSongContainedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.com/MusicOntology#tokenSongContainedIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:comment xml:lang="it">Relazione che indica i brani contenuti nel token dell&apos;album</rdfs:comment>
        <rdfs:label xml:lang="it">tokenCanzoneContenutaIn</rdfs:label>
        <rdfs:label xml:lang="en">tokenSongContainedIn</rdfs:label>
    </owl:ObjectProperty>
    


