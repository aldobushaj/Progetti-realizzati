     -->

    


    <!-- http://www.semanticweb.com/MusicOntology#artistFullName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#artistFullName">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Nome artista completo (nome e cognome)</rdfs:comment>
        <rdfs:label xml:lang="en">artist Full Name</rdfs:label>
        <rdfs:label xml:lang="it">nomeArtista Completo</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#artistNationality -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#artistNationality">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Indica la nazionalità degli artisti</rdfs:comment>
        <rdfs:label xml:lang="en">artistNationality</rdfs:label>
        <rdfs:label xml:lang="it">nazionalità Artista</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#birthDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#birthDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Indica la data di nascita di un artista</rdfs:comment>
        <rdfs:label xml:lang="en">birthDate</rdfs:label>
        <rdfs:label xml:lang="it">data Nascita</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#deathDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#deathDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="it">Indica la data di morte di un artista</rdfs:comment>
        <rdfs:label xml:lang="it">data Morte</rdfs:label>
        <rdfs:label xml:lang="en">deathDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#exemplarNumber -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#exemplarNumber">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="it">Ogni opera musicale ha una serie di token corrispondenti che può essere limitata o meno. Nel casio sia limitata abbiamo un numero limitato di token per quella determinata opera. Questo attributo indica il numero di serie, con valore 1 indica la prima copia della serie, 2 la seconda copia della serie, ecc.</rdfs:comment>
        <rdfs:label xml:lang="it">esemplareNumero</rdfs:label>
        <rdfs:label xml:lang="en">exemplarNumber</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#idToken -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#idToken">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="it">Identificativo univoco del NFT sulla blockchain</rdfs:comment>
        <rdfs:label xml:lang="en">idToken</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#nftName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#nftName">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Indica il nome del NFT</rdfs:comment>
        <rdfs:label xml:lang="en">nftName</rdfs:label>
        <rdfs:label xml:lang="it">nome NFT</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#nftValue -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#nftValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="it">Indica il valore in EUR del token</rdfs:comment>
        <rdfs:label xml:lang="en">nftValue</rdfs:label>
        <rdfs:label xml:lang="it">valore NFT</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#stageName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#stageName">
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Nome d&apos;arte dell&apos;artista</rdfs:comment>
        <rdfs:label xml:lang="it">nome d&apos;arte</rdfs:label>
        <rdfs:label xml:lang="en">stageName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#surnameUser -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#surnameUser">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Indica il cognome dell&apos;utente registrato nella piattaforma</rdfs:comment>
        <rdfs:label xml:lang="it">cognome Utente</rdfs:label>
        <rdfs:label xml:lang="en">surname_User</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.com/MusicOntology#userName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.com/MusicOntology#userName">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.com/MusicOntology#User"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Indica il nome dell&apos;utente registrato nella piattaforma</rdfs:comment>
        <rdfs:label xml:lang="it">nome Utente</rdfs:label>
        <rdfs:label xml:lang="en">user Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/vocab/frbr/core#CorporateBody -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#CorporateBody">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">La classe ente rappresenta organizzazioni o gruppi di individui, identificati da un nome</rdfs:comment>
        <rdfs:label xml:lang="en">Corporate Body</rdfs:label>
        <rdfs:label xml:lang="it">Ente</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#Expression -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Expression">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#realizerOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">È un’entità astratta, corrisponde alla forma intellettuale (o artistica) che l’opera assume per poter essere comunicata ad altri (testo, musica, immagine, digitalizzazione, ecc.).
In particolare viene astratta l&apos;opera, rendendo possibile l&apos;acquisto da parte dall&apos;utente del token corrispondente al brano musicale.</rdfs:comment>
        <rdfs:label xml:lang="it">Espressione</rdfs:label>
        <rdfs:label xml:lang="en">Expression</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#Item -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Item">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#owenerOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:comment xml:lang="it">Corrisponde al singolo esemplare in cui si materializza una manifestazione.</rdfs:comment>
        <rdfs:label xml:lang="en">Item</rdfs:label>
        <rdfs:label xml:lang="it">Item</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#Manifestation -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Manifestation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#producerOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">È un’entità concreta, corrisponde alla materializzazione dell’espressione dell&apos;opera in una determinata forma (cartacea, magnetica, elettronica, ecc.), che viene rappresentata dall&apos;item.</rdfs:comment>
        <rdfs:label xml:lang="en">Manifestation</rdfs:label>
        <rdfs:label xml:lang="it">Manifestazione</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#Person -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Person">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
        <rdfs:comment xml:lang="it">La classe Persona rappresenta individui che agiscono come singolo</rdfs:comment>
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <rdfs:label xml:lang="it">Persona</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#ResponsibleEntity -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#ResponsibleEntity">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#creator"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/vocab/frbr/core#CorporateBody"/>
            <rdf:Description rdf:about="http://purl.org/vocab/frbr/core#Person"/>
        </owl:disjointUnionOf>
        <rdfs:comment xml:lang="it">Una classe Entità Responsabile rappresenta i responsabili del contenuto intellettuale o artistico, della produzione e diffusione fisica o della custodia di qualsiasi impresa.</rdfs:comment>
        <rdfs:label xml:lang="it">Entità Responsabile</rdfs:label>
        <rdfs:label xml:lang="en">Responsable Entity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/vocab/frbr/core#Work -->

    <owl:Class rdf:about="http://purl.org/vocab/frbr/core#Work">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#creatorOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/vocab/frbr/core#ResponsibleEntity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#Album"/>
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#Composition"/>
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#Song"/>
        </owl:disjointUnionOf>
        <rdfs:comment xml:lang="it">È un’entità astratta, rappresenta il contenuto intellettuale (o artistico) come si è formato nella mente del suo autore. Si distingue dalle altre opere perché il suo contenuto intellettuale (o artistico) è diverso da quello delle altre opere.</rdfs:comment>
        <rdfs:label xml:lang="en">(Work) Opera</rdfs:label>
        <rdfs:label xml:lang="it">(Work) Opera</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.com/MusicOntology#libraryContains"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.com/MusicOntology#NFTOpera"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="it">Classe che rappresenta la  libreria Musciale di un determinato utente registrato alla piattaforma.</rdfs:comment>
        <rdfs:label xml:lang="it">LibreriaMusicale</rdfs:label>
        <rdfs:label xml:lang="en">Music Library</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.com/MusicOntology#Album -->

    <owl:Class rdf:about="http://www.semanticweb.com/MusicOntology#Album">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.com/MusicOntology#Song"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:comment xml:lang="it">Classe che rappresenta una raccolta di brani musicale. Raccolta che forma l&apos;album dell&apos;autore.</rdfs:comment>
        <rdfs:label xml:lang="en">Album</rdfs:label>
        <rdfs:label xml:lang="it">Album</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.com/MusicOntology#Artist -->

    <owl:Class rdf:about="http://www.semanticweb.com/MusicOntology#Artist">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Person"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#Composer"/>
            <rdf:Description rdf:about="http://www.semanticweb.com/MusicOntology#Singer"/>
        </owl:disjointUnionOf>
        <rdfs:comment xml:lang="it">Una classe Artista  rappresenta una tipologia specifica della classe Persona. In questo caso viene rappresentato un qualsiasi artista.</rdfs:comment>
        <rdfs:label xml:lang="en">Artist</rdfs:label>
        <rdfs:label xml:lang="it">Artista</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.com/MusicOntology#ArtistInActivity -->

    <owl:Class rdf:about="http://www.semanticweb.com/MusicOntology#ArtistInActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.com/MusicOntology#Artist"/>
        <rdfs:comment xml:lang="it">Rappresenta un artista ancora in attività, che quindi può ancora produrre opere musicali.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtistInActivity</rdfs:label>
        <rdfs:label xml:lang="it">Artista in attività</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.com/MusicOntology#Composer -->

    <owl:Class rdf:about="http://www.semanticweb.com/MusicOntology#Composer">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.com/MusicOntology#composes"/>
