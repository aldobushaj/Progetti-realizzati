<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.com/MusicOntology"
     xml:base="http://www.semanticweb.com/MusicOntology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:frbr="http://purl.org/vocab/frbr/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="http://www.semanticweb.com/MusicOntology#vann:"
     xmlns:music="http://www.semanticweb.com/MusicOntology#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:pattern="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.com/MusicOntology">
        <dc:creator xml:lang="en">Aldo Bushaj</dc:creator>
        <dc:date xml:lang="en">June 2022</dc:date>
        <dc:description xml:lang="it">L&apos;ontologia Music rappresenta la la base di conoscenza di un una piattaforma dove è possibile l&apos;acquisto di NFT legati ad opere musicali e la pubblicazione da parte degli artisti, degli NFT di una determianta opera musicale. Questa ontologia incorpora la risorsa FRBR la quale permette di legare gli artisti con le relative opere create, incorpora anche parte del vocabolario FOAF per la rappresentazione di un utente nella piattaforma.</dc:description>
        <dc:title xml:lang="en">Music Ontology</dc:title>
        <vann:preferredNamespacePrefix xml:lang="en">MusicOntology</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri xml:lang="en">http://www.semanticweb.com/MusicOntology</vann:preferredNamespaceUri>
        <rdfs:label xml:lang="en">music</rdfs:label>
    </owl:Ontology>
    


