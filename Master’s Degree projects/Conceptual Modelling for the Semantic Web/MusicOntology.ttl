@prefix : <http://www.semanticweb.com/MusicOntology> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix frbr: <http://purl.org/vocab/frbr/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://www.semanticweb.com/MusicOntology#vann:> .
@prefix music: <http://www.semanticweb.com/MusicOntology#> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix pattern: <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#> .
@base <http://www.semanticweb.com/MusicOntology> .

<http://www.semanticweb.com/MusicOntology> rdf:type owl:Ontology ;
                                            dc:creator "Aldo Bushaj"@en ;
                                            dc:date "June 2022"@en ;
                                            dc:description "L'ontologia Music rappresenta la la base di conoscenza di un una piattaforma dove è possibile l'acquisto di NFT legati ad opere musicali e la pubblicazione da parte degli artisti, degli NFT di una determianta opera musicale. Questa ontologia incorpora la risorsa FRBR la quale permette di legare gli artisti con le relative opere create, incorpora anche parte del vocabolario FOAF per la rappresentazione di un utente nella piattaforma."@it ;
                                            dc:title "Music Ontology"@en ;
                                            vann:preferredNamespacePrefix "MusicOntology"@en ;
                                            vann:preferredNamespaceUri "http://www.semanticweb.com/MusicOntology"@en ;
                                            rdfs:label "music"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.com/MusicOntology#vann:preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.com/MusicOntology#vann:preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/vocab/frbr/core#creator
frbr:creator rdf:type owl:ObjectProperty ;
             owl:inverseOf frbr:creatorOf ;
             rdfs:domain frbr:ResponsibleEntity ;
             rdfs:range frbr:Work ;
             rdfs:comment "Lega l'entita all'opera, cioè chi realizza quell'opera"@it ;
             rdfs:label "creator"@en .


###  http://purl.org/vocab/frbr/core#creatorOf
frbr:creatorOf rdf:type owl:ObjectProperty ;
               rdfs:domain frbr:Work ;
               rdfs:range frbr:ResponsibleEntity ;
               rdfs:comment "Indica da chi è stato creato il brano musciale"@it ;
               rdfs:label "creatorOf"@en .


###  http://purl.org/vocab/frbr/core#embodiment
frbr:embodiment rdf:type owl:ObjectProperty ;
                owl:inverseOf frbr:embodimentOf ;
                rdfs:domain frbr:Expression ;
                rdfs:range frbr:Manifestation ;
                rdfs:comment "L'espressione si materializzazione (embodiment) in una manifestazione."@it ;
                rdfs:label "embodiment"@en .


###  http://purl.org/vocab/frbr/core#embodimentOf
frbr:embodimentOf rdf:type owl:ObjectProperty ;
                  rdfs:domain frbr:Manifestation ;
                  rdfs:range frbr:Expression ;
                  rdfs:comment "Rappresenta la tokenizzazione astratta di un'opera artistica."@it ;
                  rdfs:label "embodimentOf"@en .


###  http://purl.org/vocab/frbr/core#exemplar
frbr:exemplar rdf:type owl:ObjectProperty ;
              owl:inverseOf frbr:exemplarOf ;
              rdfs:domain frbr:Manifestation ;
              rdfs:range frbr:Item ;
              rdfs:comment "Il singolo esemplare di una manifestazione è l'item."@it ;
              rdfs:label "exemplar"@en .


###  http://purl.org/vocab/frbr/core#exemplarOf
frbr:exemplarOf rdf:type owl:ObjectProperty ;
                rdfs:domain frbr:Item ;
                rdfs:range frbr:Manifestation ;
                rdfs:comment "L'item è il singolo esemplare di una manifestazione"@it ;
                rdfs:label "exemplarOf"@en .


###  http://purl.org/vocab/frbr/core#isAbstracted
frbr:isAbstracted rdf:type owl:ObjectProperty ;
                  owl:inverseOf frbr:itIsAnAbstraction ;
                  rdfs:domain frbr:Work ;
                  rdfs:range frbr:Expression ;
                  rdfs:comment "Indica l'opera in cosa si astrae (espressione)"@it ;
                  rdfs:label "isAbstracted"@en .


###  http://purl.org/vocab/frbr/core#itIsAnAbstraction
frbr:itIsAnAbstraction rdf:type owl:ObjectProperty ;
                       rdfs:domain frbr:Expression ;
                       rdfs:range frbr:Work ;
                       rdfs:comment "Indica l'opera che quell'espressione sta astraendo."@it ;
                       rdfs:label "itIsAnAbstraction"@en .


###  http://purl.org/vocab/frbr/core#owener
frbr:owener rdf:type owl:ObjectProperty ;
            owl:inverseOf frbr:owenerOf ;
            rdfs:domain frbr:ResponsibleEntity ;
            rdfs:range frbr:Item ;
            rdfs:comment "Indica quali item possiede l'entità responsabile"@it ;
            rdfs:label "owener"@en .


###  http://purl.org/vocab/frbr/core#owenerOf
frbr:owenerOf rdf:type owl:ObjectProperty ;
              rdfs:domain frbr:Item ;
              rdfs:range frbr:ResponsibleEntity ;
              rdfs:comment "Indica da quale entità responsabilel'item è posseduto"@it ;
              rdfs:label "owenerOf"@en .


###  http://purl.org/vocab/frbr/core#producer
frbr:producer rdf:type owl:ObjectProperty ;
              owl:inverseOf frbr:producerOf ;
              rdfs:domain frbr:ResponsibleEntity ;
              rdfs:range frbr:Manifestation ;
              rdfs:comment "Indica quale manifestazione l'entità responsabile ha prodotto"@it ;
              rdfs:label "producer"@en .


###  http://purl.org/vocab/frbr/core#producerOf
frbr:producerOf rdf:type owl:ObjectProperty ;
                rdfs:domain frbr:Manifestation ;
                rdfs:range frbr:ResponsibleEntity ;
                rdfs:comment "Indica la manifestazione da quale entità responsabile è stata prodotta"@it ;
                rdfs:label "producerOf"@en .


###  http://purl.org/vocab/frbr/core#realizer
frbr:realizer rdf:type owl:ObjectProperty ;
              owl:inverseOf frbr:realizerOf ;
              rdfs:domain frbr:ResponsibleEntity ;
              rdfs:range frbr:Expression ;
              rdfs:comment "Indica l'espressione realizzata dall'entità responsabile"@it ;
              rdfs:label "realizer"@en .


###  http://purl.org/vocab/frbr/core#realizerOf
frbr:realizerOf rdf:type owl:ObjectProperty ;
                rdfs:domain frbr:Expression ;
                rdfs:range frbr:ResponsibleEntity ;
                rdfs:comment "Indica da quale entità responsabile è stata realizzata l'espressione"@it ;
                rdfs:label "realizerOf"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart
pattern:hasPart rdf:type owl:ObjectProperty ;
                owl:inverseOf pattern:isPartOf ;
                rdfs:domain music:Album ;
                rdfs:range music:Song ;
                rdfs:comment "Relazione che lega l'album al brano musicale. Indica quindi da quali brani musicali è composto l'album"@it ;
                rdfs:label "hasPart"@en ,
                           "èCompostoDa"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf
pattern:isPartOf rdf:type owl:ObjectProperty ;
                 rdfs:domain music:Song ;
                 rdfs:range music:Album ;
                 rdfs:comment "Relazione che lega il brano musicale all'album a cui appartiene."@it ;
                 rdfs:label "faParteDi"@it ,
                            "isPartOf"@en .


###  http://www.semanticweb.com/MusicOntology#albumOf
music:albumOf rdf:type owl:ObjectProperty ;
              owl:inverseOf music:hasAlbum ;
              rdfs:domain music:Album ;
              rdfs:range frbr:ResponsibleEntity ;
              rdfs:comment "Indica a quale artista appartiene un determinato album"@it ;
              rdfs:label "albumDi"@it ,
                         "albumOf"@en .


###  http://www.semanticweb.com/MusicOntology#composedBy
music:composedBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf frbr:creatorOf ;
                 owl:inverseOf music:composes ;
                 rdfs:domain music:Composition ;
                 rdfs:range music:Composer ;
                 rdfs:comment "Lega la composizione con il compositore. Indica dunque da chi è stato composta una composizione"@it ;
                 rdfs:label "composedBy"@en ,
                            "compostaDa"@it .


###  http://www.semanticweb.com/MusicOntology#composes
music:composes rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf frbr:creator ;
               rdfs:domain music:Composer ;
               rdfs:range music:Composition ;
               owl:propertyDisjointWith music:sing ;
               rdfs:comment "Artista compositore che compone un brano musciale"@it ;
               rdfs:label "compone"@it ,
                          "composes"@en .


###  http://www.semanticweb.com/MusicOntology#consistOf
music:consistOf rdf:type owl:ObjectProperty ;
                owl:inverseOf music:formAGroup ;
                rdfs:domain music:Group ;
                rdfs:range music:Artist ;
                rdfs:comment "Specifica da quali artisti è composto il gruppo"@it ;
                rdfs:label "consistOf"@en ,
                           "formatoDa"@it .


###  http://www.semanticweb.com/MusicOntology#containedInLibrary
music:containedInLibrary rdf:type owl:ObjectProperty ;
                         owl:inverseOf music:libraryContains ;
                         rdfs:domain music:NFTOpera ;
                         rdfs:range pattern:MusicLibrary ;
                         rdfs:comment "Il token di un'opera è contenuto in una libreria musicale."@it ;
                         rdfs:label "containedInLibrary"@en ,
                                    "contenutoInLibreria"@it .


###  http://www.semanticweb.com/MusicOntology#featuring
music:featuring rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain music:Singer ;
                rdfs:range music:Singer ;
                rdfs:comment "Lega più artisti che collaborano nella realizzazione di una canzone(featuring)"@it ;
                rdfs:label "collabora"@it ,
                           "featuring"@en .


###  http://www.semanticweb.com/MusicOntology#formAGroup
music:formAGroup rdf:type owl:ObjectProperty ;
                 rdfs:domain music:Artist ;
                 rdfs:range music:Group ;
                 rdfs:comment "Lega l'artista al gruppo musicale a cui appartiene"@it ;
                 rdfs:label "formAGroup"@en ,
                            "forma"@it .


###  http://www.semanticweb.com/MusicOntology#groupSing
music:groupSing rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf frbr:creator ;
                owl:inverseOf music:singedByGroup ;
                rdfs:domain music:Group ;
                rdfs:range music:Song ;
                rdfs:comment "Gruppo musicale che canta una canzone"@it ;
                rdfs:label "groupSing"@en ,
                           "gruppoCanta"@it .


###  http://www.semanticweb.com/MusicOntology#hasAlbum
music:hasAlbum rdf:type owl:ObjectProperty ;
               rdfs:domain frbr:ResponsibleEntity ;
               rdfs:range music:Album ;
               rdfs:comment "Indica quale album e posseduto da un artista o un gruppo composto da più artisti"@it ;
               rdfs:label "haAlbum"@it ,
                          "hasAlbum"@en .


###  http://www.semanticweb.com/MusicOntology#hasGenre
music:hasGenre rdf:type owl:ObjectProperty ;
               rdfs:domain music:Song ;
               rdfs:range music:Genre ;
               rdfs:comment "Proprietà che indica il genere della canzone"@it ;
               rdfs:label "hasGenre"@en ,
                          "èDiGenere"@it .


###  http://www.semanticweb.com/MusicOntology#hasLibrary
music:hasLibrary rdf:type owl:ObjectProperty ;
                 owl:inverseOf music:libraryOf ;
                 rdfs:domain music:User ;
                 rdfs:range pattern:MusicLibrary ;
                 rdfs:comment "Proprietà che indica la libreria musicale posseduta da un determianto utente"@it ;
                 rdfs:label "haLibreria"@it ,
                            "hasLibrary"@en .


###  http://www.semanticweb.com/MusicOntology#hasSongs
music:hasSongs rdf:type owl:ObjectProperty ;
               rdfs:domain music:Genre ;
               rdfs:range music:Song ;
               rdfs:comment "Relazione mi permette di determinare quali brani musicali appartengono ad un determinato genere musicale."@it ;
               rdfs:label "haCanzoni"@it ,
                          "hasSongs"@en .


###  http://www.semanticweb.com/MusicOntology#hasToken
music:hasToken rdf:type owl:ObjectProperty ;
               owl:inverseOf music:tokenOfUser ;
               rdfs:domain music:User ;
               rdfs:range music:NFTOpera ;
               rdfs:comment "Indica quali token un utente possiede"@it ;
               rdfs:label "haToken"@it ,
                          "hasToken"@en .


###  http://www.semanticweb.com/MusicOntology#libraryContains
music:libraryContains rdf:type owl:ObjectProperty ;
                      rdfs:domain pattern:MusicLibrary ;
                      rdfs:range music:NFTOpera ;
                      rdfs:comment "Una libreria musicale contiene un token opera, che può essere un album, un brano musicale o una composizione."@it ;
                      rdfs:label "libraryContains"@en ,
                                 "libreriaContiene"@it .


###  http://www.semanticweb.com/MusicOntology#libraryHas
music:libraryHas rdf:type owl:ObjectProperty ;
                 rdfs:domain pattern:MusicLibrary ;
                 rdfs:range music:NFTOpera ;
                 owl:propertyChainAxiom ( music:libraryContains
                                          music:tokenAlbumContains
                                        ) ;
                 rdfs:comment "Property chain: se una libreria contiene un token opera e questo token opera è un album, l'album a sua volta conterra un token brano musicale, quindi inferiamo che la libreria musicale HA quel/i brano/i musicale/i."@it ;
                 rdfs:label "libraryHas"@en ,
                            "libreriaHa"@it .


###  http://www.semanticweb.com/MusicOntology#libraryOf
music:libraryOf rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain pattern:MusicLibrary ;
                rdfs:range music:User ;
                rdfs:comment "Indica a chi appartiene la libreria musicale"@it ;
                rdfs:label "libraryOf"@en ,
                           "libreriaDi"@it .


###  http://www.semanticweb.com/MusicOntology#sing
music:sing rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf frbr:creator ;
           owl:inverseOf music:singedBy ;
           rdfs:domain music:Singer ;
           rdfs:range music:Song ;
           rdfs:comment "Artista (solista o di gruppo) che canta la melodia principale di un brano musicale"@it ;
           rdfs:label "canta"@it ,
                      "sing"@en .


###  http://www.semanticweb.com/MusicOntology#singedBy
music:singedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf frbr:creatorOf ;
               rdfs:domain music:Song ;
               rdfs:range music:Singer ;
               rdfs:comment "Ci dice da chi è stato cantato un brano musciale"@it ;
               rdfs:label "cantataDa"@it ,
                          "singedBy"@en .


###  http://www.semanticweb.com/MusicOntology#singedByGroup
music:singedByGroup rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf frbr:creatorOf ;
                    rdfs:domain music:Song ;
                    rdfs:range music:Group ;
                    rdfs:comment "Relazione che lega la canzone con il gruppo musicale, quindi chi canta quella canzone"@it ;
                    rdfs:label "cantataDaGruppo"@it ,
                               "singedByGroup"@en .


###  http://www.semanticweb.com/MusicOntology#tokenAlbumContains
music:tokenAlbumContains rdf:type owl:ObjectProperty ;
                         owl:inverseOf music:tokenSongContainedIn ;
                         rdfs:domain music:NFTOpera ;
                         rdfs:range music:NFTOpera ;
                         rdfs:comment "Relazione utilizzata solo nel caso degli album. In questo specifico caso un album contiene a sua volta il token delle canzoni."@it ;
                         rdfs:label "tokenAlbumContiene"@it ,
                                    "tokenContains"@en .


###  http://www.semanticweb.com/MusicOntology#tokenOfUser
music:tokenOfUser rdf:type owl:ObjectProperty ;
                  rdfs:domain music:NFTOpera ;
                  rdfs:range music:User ;
                  rdfs:comment "Indica uno specifico token di una canzone a quale utente appartiene"@it ;
                  rdfs:label "tokenDi"@it ,
                             "tokenOfUser"@en .


###  http://www.semanticweb.com/MusicOntology#tokenSongContainedIn
music:tokenSongContainedIn rdf:type owl:ObjectProperty ;
                           rdfs:domain music:NFTOpera ;
                           rdfs:range music:NFTOpera ;
                           rdfs:comment "Relazione che indica i brani contenuti nel token dell'album"@it ;
                           rdfs:label "tokenCanzoneContenutaIn"@it ,
                                      "tokenSongContainedIn"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.com/MusicOntology#artistFullName
music:artistFullName rdf:type owl:DatatypeProperty ;
                     rdfs:domain music:Artist ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Nome artista completo (nome e cognome)"@it ;
                     rdfs:label "artist Full Name"@en ,
                                "nomeArtista Completo"@it .


###  http://www.semanticweb.com/MusicOntology#artistNationality
music:artistNationality rdf:type owl:DatatypeProperty ;
                        rdfs:domain music:Artist ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Indica la nazionalità degli artisti"@it ;
                        rdfs:label "artistNationality"@en ,
                                   "nazionalità Artista"@it .


###  http://www.semanticweb.com/MusicOntology#birthDate
music:birthDate rdf:type owl:DatatypeProperty ;
                rdfs:domain music:Artist ;
                rdfs:range xsd:string ;
                rdfs:comment "Indica la data di nascita di un artista"@it ;
                rdfs:label "birthDate"@en ,
                           "data Nascita"@it .


###  http://www.semanticweb.com/MusicOntology#deathDate
music:deathDate rdf:type owl:DatatypeProperty ;
                rdfs:domain music:Artist ;
                rdfs:range xsd:integer ;
                rdfs:comment "Indica la data di morte di un artista"@it ;
                rdfs:label "data Morte"@it ,
                           "deathDate"@en .


###  http://www.semanticweb.com/MusicOntology#exemplarNumber
music:exemplarNumber rdf:type owl:DatatypeProperty ;
                     rdfs:domain music:NFTOpera ;
                     rdfs:range xsd:nonNegativeInteger ;
                     rdfs:comment "Ogni opera musicale ha una serie di token corrispondenti che può essere limitata o meno. Nel casio sia limitata abbiamo un numero limitato di token per quella determinata opera. Questo attributo indica il numero di serie, con valore 1 indica la prima copia della serie, 2 la seconda copia della serie, ecc."@it ;
                     rdfs:label "esemplareNumero"@it ,
                                "exemplarNumber"@en .


###  http://www.semanticweb.com/MusicOntology#idToken
music:idToken rdf:type owl:DatatypeProperty ;
              rdfs:domain music:NFTOpera ;
              rdfs:range xsd:integer ;
              rdfs:comment "Identificativo univoco del NFT sulla blockchain"@it ;
              rdfs:label "idToken"@en .


###  http://www.semanticweb.com/MusicOntology#nftName
music:nftName rdf:type owl:DatatypeProperty ;
              rdfs:domain music:NFTOpera ;
              rdfs:range xsd:string ;
              rdfs:comment "Indica il nome del NFT"@it ;
              rdfs:label "nftName"@en ,
                         "nome NFT"@it .


###  http://www.semanticweb.com/MusicOntology#nftValue
music:nftValue rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain music:NFTOpera ;
               rdfs:range xsd:nonNegativeInteger ;
               rdfs:comment "Indica il valore in EUR del token"@it ;
               rdfs:label "nftValue"@en ,
                          "valore NFT"@it .


###  http://www.semanticweb.com/MusicOntology#stageName
music:stageName rdf:type owl:DatatypeProperty ;
                rdfs:domain music:Artist ;
                rdfs:range xsd:string ;
                rdfs:comment "Nome d'arte dell'artista"@it ;
                rdfs:label "nome d'arte"@it ,
                           "stageName"@en .


###  http://www.semanticweb.com/MusicOntology#surnameUser
music:surnameUser rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain music:User ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indica il cognome dell'utente registrato nella piattaforma"@it ;
                  rdfs:label "cognome Utente"@it ,
                             "surname_User"@en .


###  http://www.semanticweb.com/MusicOntology#userName
music:userName rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain music:User ;
               rdfs:range xsd:string ;
               rdfs:comment "Indica il nome dell'utente registrato nella piattaforma"@it ;
               rdfs:label "nome Utente"@it ,
                          "user Name"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/vocab/frbr/core#CorporateBody
frbr:CorporateBody rdf:type owl:Class ;
                   rdfs:subClassOf frbr:ResponsibleEntity ;
                   rdfs:comment "La classe ente rappresenta organizzazioni o gruppi di individui, identificati da un nome"@it ;
                   rdfs:label "Corporate Body"@en ,
                              "Ente"@it .


###  http://purl.org/vocab/frbr/core#Expression
frbr:Expression rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty frbr:realizerOf ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass frbr:ResponsibleEntity
                                    ] ;
                rdfs:comment """È un’entità astratta, corrisponde alla forma intellettuale (o artistica) che l’opera assume per poter essere comunicata ad altri (testo, musica, immagine, digitalizzazione, ecc.).
In particolare viene astratta l'opera, rendendo possibile l'acquisto da parte dall'utente del token corrispondente al brano musicale."""@it ;
                rdfs:label "Espressione"@it ,
                           "Expression"@en .


###  http://purl.org/vocab/frbr/core#Item
frbr:Item rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty frbr:owenerOf ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass frbr:ResponsibleEntity
                              ] ;
          rdfs:subClassOf frbr:Manifestation ;
          rdfs:comment "Corrisponde al singolo esemplare in cui si materializza una manifestazione."@it ;
          rdfs:label "Item"@en ,
                     "Item"@it .


###  http://purl.org/vocab/frbr/core#Manifestation
frbr:Manifestation rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty frbr:producerOf ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass frbr:ResponsibleEntity
                                       ] ;
                   rdfs:comment "È un’entità concreta, corrisponde alla materializzazione dell’espressione dell'opera in una determinata forma (cartacea, magnetica, elettronica, ecc.), che viene rappresentata dall'item."@it ;
                   rdfs:label "Manifestation"@en ,
                              "Manifestazione"@it .


###  http://purl.org/vocab/frbr/core#Person
frbr:Person rdf:type owl:Class ;
            rdfs:subClassOf frbr:ResponsibleEntity ;
            rdfs:comment "La classe Persona rappresenta individui che agiscono come singolo"@it ;
            rdfs:label "Person"@en ,
                       "Persona"@it .


###  http://purl.org/vocab/frbr/core#ResponsibleEntity
frbr:ResponsibleEntity rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty frbr:creator ;
                                             owl:someValuesFrom frbr:Work
                                           ] ;
                       owl:disjointUnionOf ( frbr:CorporateBody
                                             frbr:Person
                                           ) ;
                       rdfs:comment "Una classe Entità Responsabile rappresenta i responsabili del contenuto intellettuale o artistico, della produzione e diffusione fisica o della custodia di qualsiasi impresa."@it ;
                       rdfs:label "Entità Responsabile"@it ,
                                  "Responsable Entity"@en .


###  http://purl.org/vocab/frbr/core#Work
frbr:Work rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty frbr:creatorOf ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass frbr:ResponsibleEntity
                              ] ;
          owl:disjointUnionOf ( music:Album
                                music:Composition
                                music:Song
                              ) ;
          rdfs:comment "È un’entità astratta, rappresenta il contenuto intellettuale (o artistico) come si è formato nella mente del suo autore. Si distingue dalle altre opere perché il suo contenuto intellettuale (o artistico) è diverso da quello delle altre opere."@it ;
          rdfs:label "(Work) Opera"@en ,
                     "(Work) Opera"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#MusicLibrary
pattern:MusicLibrary rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty music:libraryContains ;
                                           owl:someValuesFrom music:NFTOpera
                                         ] ;
                     rdfs:comment "Classe che rappresenta la  libreria Musciale di un determinato utente registrato alla piattaforma."@it ;
                     rdfs:label "LibreriaMusciale"@it ,
                                "Music Library"@en .


###  http://www.semanticweb.com/MusicOntology#Album
music:Album rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty pattern:hasPart ;
                                  owl:someValuesFrom music:Song
                                ] ;
            rdfs:subClassOf frbr:Work ;
            rdfs:comment "Classe che rappresenta una raccolta di brani musicale. Raccolta che forma l'album dell'autore."@it ;
            rdfs:label "Album"@en ,
                       "Album"@it .


###  http://www.semanticweb.com/MusicOntology#Artist
music:Artist rdf:type owl:Class ;
             rdfs:subClassOf frbr:Person ;
             owl:disjointUnionOf ( music:Composer
                                   music:Singer
                                 ) ;
             rdfs:comment "Una classe Artista  rappresenta una tipologia specifica della classe Persona. In questo caso viene rappresentato un qualsiasi artista."@it ;
             rdfs:label "Artist"@en ,
                        "Artista"@it .


###  http://www.semanticweb.com/MusicOntology#ArtistInActivity
music:ArtistInActivity rdf:type owl:Class ;
                       rdfs:subClassOf music:Artist ;
                       rdfs:comment "Rappresenta un artista ancora in attività, che quindi può ancora produrre opere musicali."@it ;
                       rdfs:label "ArtistInActivity"@en ,
                                  "Artista in attività"@it .


###  http://www.semanticweb.com/MusicOntology#Composer
music:Composer rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty music:composes ;
                                     owl:someValuesFrom music:Composition
                                   ] ;
               rdfs:subClassOf music:Artist ;
               rdfs:comment "Una classe Compositore rappresenta una tipologia specifica della classe Artista. In questo caso viene rappresentato un qualsiasi compositore di musica."@it ;
               rdfs:label "Composer"@en ,
                          "Compositore"@it .


###  http://www.semanticweb.com/MusicOntology#Composition
music:Composition rdf:type owl:Class ;
                  rdfs:subClassOf frbr:Work ;
                  rdfs:comment "Con la classe Composizone viene rappresentato una qualsiasi composizione."@it ;
                  rdfs:label "Composition"@en ,
                             "Composizione"@it .


###  http://www.semanticweb.com/MusicOntology#Expert
music:Expert rdf:type owl:Class ;
             rdfs:subClassOf music:User ;
             rdfs:comment "Classe che rappresenta gli esperti musicali, cioè coloro che hannno nella loro libreria musicale il token di una composizione"@it ;
             rdfs:label "Esperto Musicale"@it ,
                        "Expert"@en .


###  http://www.semanticweb.com/MusicOntology#Genre
music:Genre rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( music:Pop
                                              music:Reggaeton
                                              music:Space_Rock
                                              <http://www.semanticweb.com/MusicOntology#Dance/Elettronica>
                                            )
                                ] ;
            rdfs:comment "Classe che rappresenta un genere musicale. Determina quindi il genere musicale del brano a cui è associato."@it ;
            rdfs:label "Genere"@it ,
                       "Genre"@en .


###  http://www.semanticweb.com/MusicOntology#Group
music:Group rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty music:consistOf ;
                                  owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                  owl:onClass music:Artist
                                ] ;
            rdfs:subClassOf frbr:CorporateBody ;
            rdfs:comment "Con la classe Gruppo rappresentiamo più individui che formano un gruppo musicale."@it ;
            rdfs:label "Group"@en ,
                       "GruppoMusicale"@it .


###  http://www.semanticweb.com/MusicOntology#NFTOpera
music:NFTOpera rdf:type owl:Class ;
               rdfs:subClassOf frbr:Item ;
               rdfs:comment "Una classe NFT opera rappresenta un Token Non Fungibile che rappresenta una determianta opera musicale. Possiamo avere una serie limitata di Token Non Fungibili."@it ;
               rdfs:label "NFT Opera"@en ,
                          "NFT Opera"@it .


###  http://www.semanticweb.com/MusicOntology#Singer
music:Singer rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty music:sing ;
                                   owl:someValuesFrom music:Song
                                 ] ;
             rdfs:subClassOf music:Artist ;
             rdfs:comment "Con la classe Cantante vengono rappresentati i cantanti."@it ;
             rdfs:label "Cantante"@it ,
                        "Singer"@en .


###  http://www.semanticweb.com/MusicOntology#Song
music:Song rdf:type owl:Class ;
           rdfs:subClassOf frbr:Work ;
           rdfs:comment "Con la classe Brano Musicale viene rappresentato un qualsiasi brano musicale."@it ;
           rdfs:label "Brano Musicale"@it ,
                      "Song"@en .


###  http://www.semanticweb.com/MusicOntology#UncommonNFT
music:UncommonNFT rdf:type owl:Class ;
                  rdfs:subClassOf music:NFTOpera ;
                  rdfs:comment """Rappresenta un token speciale in quanto, il suo valore, è molto maggiore rispetto agli altri token rappresentanti la stessa opera.
Si è visto che più il token si avvicina al numero 1, più questi acquistano valore, con il token #1 che adirittura ha valori altissimi. Questo perché il token #1 ha la particolarità di essere il primo token coniato, rappresentate quella determinata opera."""@it ;
                  rdfs:label "NFT Raro"@it ,
                             "UncommonNFT"@en .


###  http://www.semanticweb.com/MusicOntology#User
music:User rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty music:hasLibrary ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass pattern:MusicLibrary
                               ] ;
           rdfs:subClassOf foaf:Person ;
           rdfs:comment "Una classe Utente corrisponde al profilo di una determinata Persona."@it ;
           rdfs:label "User"@en ,
                      "Utente"@it .


